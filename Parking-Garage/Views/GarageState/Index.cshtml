@{
    ViewData["Title"] = "Garage State Page";
}
   
<link rel="stylesheet" href="./css/main.css">
<link rel="stylesheet" href="./css/check-in.css">
<link rel="stylesheet" href="./css/garage-state.css">

<div class="page">
    <div class="header-div"> 
        <h1 class="header-page"> pick my car </h1>
        <img class="page-picture" src="../images/check-out-car.jpeg" alt="checl out car">
    </div>
    <div class="container">
        <table class="versions-table" border = 1>
                    <thead>
                         <tr class="first-row">
                            <th>slot</th>
                            <th>plateID</th>
                          </tr>
                    </thead>
                    @foreach (var i in ViewBag.Data)
                    {
                        <div class="row-div">
                            <tr>
                                <th>  @i.Key  </th>
                                <th> @i.Value </th>                           
                            </tr>
                        </div>
                    }
       </table>
    </div>
    
    <input class="simple-button" type = "button" onclick="openWindow('http://localhost:5001/')" value="return to main page"/>
    
</div>
  
  <script src="../js/site.js"  type="text/javascript"> </script>